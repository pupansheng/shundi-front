<view class="content"><block wx:if="{{entity.usertype==''}}"><view><text style="color:red;align-content:center;">请先选择注册用户类型！</text></view></block><block wx:if="{{entity.usertype==1}}"><view><q-s-input bind:input="__e" vue-id="1" name="{{name}}" titleColor="#f1505c" variableName="input" required layout="column" title="qq邮箱" value="{{entity.bk1}}" data-event-opts="{{[['^input',[['__set_model',['$0','bk1','$event',[]],['entity']]]]]}}" bind:__l="__l"></q-s-input><q-s-input bind:input="__e" vue-id="2" name="{{name}}" titleColor="#f1505c" variableName="input" required layout="column" title="真实姓名" value="{{entity.realname}}" data-event-opts="{{[['^input',[['__set_model',['$0','realname','$event',[]],['entity']]]]]}}" bind:__l="__l"></q-s-input><q-s-input bind:input="__e" vue-id="3" name="{{name}}" titleColor="#f1505c" variableName="input" required layout="column" title="户籍" value="{{entity.nativeplace}}" data-event-opts="{{[['^input',[['__set_model',['$0','nativeplace','$event',[]],['entity']]]]]}}" bind:__l="__l"></q-s-input><q-s-input bind:input="__e" vue-id="4" name="{{name}}" titleColor="#f1505c" variableName="input" required layout="column" title="现居地址" value="{{entity.nowplace}}" data-event-opts="{{[['^input',[['__set_model',['$0','nowplace','$event',[]],['entity']]]]]}}" bind:__l="__l"></q-s-input><q-s-pics vue-id="5" name="{{name}}" variableName="pics" imageUrl="{{serverUrl+'/'+user.idcardimage1}}" exists="{{user.idcardimage1}}" title="身份证正面" typeName="idcardimage1" value="{{idcardimage1}}" data-event-opts="{{[['^upload',[['uploadImage']]],['^input',[['__set_model',['','idcardimage1','$event',[]]]]]]}}" bind:upload="__e" bind:input="__e" bind:__l="__l"></q-s-pics><q-s-pics vue-id="6" name="{{name}}" variableName="pics" imageUrl="{{serverUrl+'/'+user.idcardimage2}}" exists="{{user.idcardimage2}}" title="身份证反面" typeName="idcardimage2" value="{{idcardimage2}}" data-event-opts="{{[['^upload',[['uploadImage']]],['^input',[['__set_model',['','idcardimage2','$event',[]]]]]]}}" bind:upload="__e" bind:input="__e" bind:__l="__l"></q-s-pics><button type="primary" data-event-opts="{{[['tap',[['toSave']]]]}}" bindtap="__e">{{user.status!=1?'审核资料':'重新审核'}}</button></view></block></view>