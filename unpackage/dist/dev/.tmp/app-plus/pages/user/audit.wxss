
.page {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	background: url(../../static/img/index.a8f80981.jpg) no-repeat center;
	background-size: cover;
}
.page .alert {
	position: fixed;
	top: 0;
	right: 0;
	bottom: 0;
	left: 0;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background: rgba(0, 0, 0, 0.4);
}
.page .alert .main {
	position: relative;
	display: none;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding: 50rpx;
	border-radius: 12rpx;
	width: 480rpx;
	height: 360rpx;
	background: rgba(255, 255, 255, 0);
}
.page .alert .main .icon {
	position: absolute;
	top: -48rpx;
	width: 98rpx;
	height: 98rpx;
	background: url(data:image/png;base64,) no-repeat;
	background-size: contain;
}
.page .alert .main .text {
	padding: 50rpx 0;
	color: #4c4e60;
	font-size: 32rpx;
}
.page .alert .main .code {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 30rpx 0 50rpx;
	width: 360rpx;
}
.page .alert .main .code input {
	width: 40rpx;
	height: 40rpx;
	line-height: 40rpx;
	border: none;
	border-bottom: 4rpx solid #b2bfbd;
	text-align: center;
	color: #4c4e60;
	font-size: 46rpx;
}
.page .alert .main .code input.focus {
	border-color: #4c79fa;
}
.page .alert .main .btns {
	width: 360rpx;
	height: 80rpx;
	line-height: 80rpx;
	border-radius: 40rpx;
	text-align: center;
	color: white;
	font-size: 32rpx;
	font-weight: bold;
	background: -webkit-linear-gradient(#614cff, #4c79fa);
	background: linear-gradient(#614cff, #4c79fa);
}
@-webkit-keyframes show {
0% {
		-webkit-transform: scale(0);
		        transform: scale(0);
		background: rgba(255, 255, 255, 0.3);
}
50% {
		-webkit-transform: scale(1.1);
		        transform: scale(1.1);
		background: rgba(255, 255, 255, 0.6);
}
100% {
		-webkit-transform: scale(1);
		        transform: scale(1);
		background: rgba(255, 255, 255, 1);
}
}
@keyframes show {
0% {
		-webkit-transform: scale(0);
		        transform: scale(0);
		background: rgba(255, 255, 255, 0.3);
}
50% {
		-webkit-transform: scale(1.1);
		        transform: scale(1.1);
		background: rgba(255, 255, 255, 0.6);
}
100% {
		-webkit-transform: scale(1);
		        transform: scale(1);
		background: rgba(255, 255, 255, 1);
}
}
.page .alert .show {
	display: -webkit-box !important;
	display: -webkit-flex !important;
	display: flex !important;
	-webkit-animation: show 0.6s ease-in-out forwards;
	        animation: show 0.6s ease-in-out forwards;
}

