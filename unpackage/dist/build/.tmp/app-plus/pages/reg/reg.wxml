<view><cmd-page-body vue-id="1" type="top" bind:__l="__l" vue-slots="{{['default']}}"><view class="register"><view class="register-title">{{status?'手机快捷注册':'账号密码注册'}}</view><view class="register-explain">{{status?'用户可通过手机验证码直接注册':'使用账号密码注册后请绑定手机号'}}</view><block wx:if="{{status}}"><cmd-transition vue-id="{{('2')+','+('1')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><view class="register-phone"><cmd-input bind:input="__e" vue-id="{{('3')+','+('2')}}" type="number" focus maxlength="11" placeholder="请输入手机号" value="{{mobile.phone}}" data-event-opts="{{[['^input',[['__set_model',['$0','phone','$event',[]],['mobile']]]]]}}" bind:__l="__l"></cmd-input><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="register-phone-getcode" bindtap="__e">{{!safety.state&&'获取验证码'||safety.time+' s'}}</view></view><view class="register-code"><cmd-input bind:input="__e" vue-id="{{('4')+','+('2')}}" type="number" maxlength="6" placeholder="请输入短信验证码" value="{{mobile.code}}" data-event-opts="{{[['^input',[['__set_model',['$0','code','$event',[]],['mobile']]]]]}}" bind:__l="__l"></cmd-input></view><button class="{{['btn-register',registerMobile?'btn-register-active':'']}}" hover-class="btn-register-hover" data-event-opts="{{[['tap',[['fnRegister',['$event']]]]]}}" bindtap="__e">注册</button></cmd-transition></block><block wx:if="{{!status}}"><cmd-transition vue-id="{{('5')+','+('1')}}" name="fade-up" bind:__l="__l" vue-slots="{{['default']}}"><view class="register-username"><cmd-input bind:input="__e" vue-id="{{('6')+','+('5')}}" type="text" focus maxlength="26" placeholder="请输入账号" value="{{account.username}}" data-event-opts="{{[['^input',[['__set_model',['$0','username','$event',[]],['account']]]]]}}" bind:__l="__l"></cmd-input></view><view class="register-password"><cmd-input bind:input="__e" vue-id="{{('7')+','+('5')}}" type="password" displayable maxlength="26" placeholder="请输入密码" value="{{account.password}}" data-event-opts="{{[['^input',[['__set_model',['$0','password','$event',[]],['account']]]]]}}" bind:__l="__l"></cmd-input></view><button class="{{['btn-register',registerAccount?'btn-register-active':'']}}" hover-class="btn-register-hover" data-event-opts="{{[['tap',[['fnRegister',['$event']]]]]}}" bindtap="__e">注册</button></cmd-transition></block></view></cmd-page-body></view>